    "\n",
    "    print(\"Please enter a positive number\")\n",
    "\n",
    "else:\n",
    "\n",
    "        while i <= num:\n",
    "\n",
    "               result += i\n",
    "\n",
    "               i += 1\n",
    "\n",
    "        print(\"The sum is\", result)\n",
    "\n"
